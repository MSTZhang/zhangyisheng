		<td height="20" bgcolor="#FFFFFF"><div align="center" class="STYLE21"><span><%=datas[i].adress%></span></div></td>
		<td height="20" bgcolor="#FFFFFF"><div align="center" class="STYLE21"><span><%=datas[i].days%></span></div></td>
		<td height="20" bgcolor="#FFFFFF"><div align="center" class="STYLE21"><a href="javascript:void(0)" style="text-decoration: none;color:green;">删除</a> | <%if(datas[i].status==1){%><a href="javascript:void(0)" class="checkBtn" style="text-decoration: none;color:green;">结账</a><%}else if(datas[i].status==2){%><a style="text-decoration: none;color:grey;">结账</a><%}%></div></td>
	</tr>
	<%}%>
	<script type="text/javascript">
		$(".edit").click(function(){
			alert("编辑");
			editText();
		});
		$(".save").click(function(){
			
		});
		$(".checkBtn").click(function(){
			alert("确认结账吗");
			var checkNum = $(this).parent().parent().parent().first().children().children().children("#checkNum").val();
			var url1 = "houseList.html?method=balance&value="+checkNum;
			window.location.href=url1;
		});
