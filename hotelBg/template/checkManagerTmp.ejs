		<td height="20" bgcolor="#FFFFFF" class="STYLE19"><div align="center"><span class="STYLE22"><%=datas[i].sex%></span></div></td>
		<td height="20" bgcolor="#FFFFFF" class="STYLE19"><div align="center" class="STYLE22"><span><%=datas[i].checkTime%></span></div></td>
